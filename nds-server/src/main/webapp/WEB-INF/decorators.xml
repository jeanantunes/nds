<?xml version="1.0" encoding="UTF-8"?>

<decorators defaultdir="/WEB-INF/jsp/decorators">

    <excludes> 
        <pattern>/images/*</pattern> 
        <pattern>/scripts/*</pattern> 
        <pattern>/css/*</pattern>
        <pattern>/upload/*</pattern>
        <pattern>**impriNotaPromissoria*</pattern>
        <pattern>**/mapaAbastecimento/imprimir*</pattern>
        
    </excludes>
              
    <decorator name="main" page="main.jsp">
        <pattern>/*</pattern>
    </decorator>
    
     <decorator name="dialog" page="dialog.jsp">
        <pattern>/*Dialog.jsp</pattern>
    </decorator>
    
</decorators>
